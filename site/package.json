{
	"name": "nuxt-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm generate && nuxt build",
		"dev": "nuxt dev",
		"generate": "prisma generate && nuxt generate && pnpm cf-typegen",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"prisma:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate:create": "node scripts/db-migrate.mjs create",
		"db:migrate:generate": "prisma migrate diff --from-config-datasource --to-schema ./prisma/schema.prisma --script --output",
		"db:migrate:local": "node scripts/db-migrate.mjs apply --local",
		"db:migrate:remote": "node scripts/db-migrate.mjs apply --remote",
		"db:migrate:status": "node scripts/db-migrate.mjs list",
		"db:migrate:status:remote": "node scripts/db-migrate.mjs list --remote",
		"deploy": "pnpm build && wrangler deploy",
		"cf-typegen": "wrangler types",
		"typecheck": "nuxt typecheck"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.940.0",
		"@aws-sdk/s3-request-presigner": "^3.940.0",
		"@breezystack/lamejs": "^1.2.7",
		"@nuxtjs/i18n": "10.2.1",
		"@orpc/client": "^1.11.3",
		"@orpc/contract": "^1.11.3",
		"@orpc/server": "^1.11.3",
		"@prisma/adapter-d1": "^7.0.1",
		"@prisma/client": "7.0.1",
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/vue-store": "^0.8.0",
		"dotenv": "^17.2.3",
		"h3": "^1.13.0",
		"jose": "^6.1.2",
		"nitro-cloudflare-dev": "^0.2.2",
		"nuxt": "^4.2.1",
		"tailwindcss": "^4.1.17",
		"uuid": "^13.0.0",
		"vue": "^3.5.25",
		"vue-router": "^4.6.3",
		"vue-tsc": "^3.1.5"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@vue/language-server": "^3.1.5",
		"prisma": "^7.0.1"
	},
	"packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
